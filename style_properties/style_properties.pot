msgid "``\"step\"``"
msgstr ""

#: ../../source/style_properties.rst:610
msgid "The default, best for text that uses thin outlines and text that uses multiple outlines. The window scaling factor is rounded down to an integer and applied ot the outline size and offsets."
msgstr ""

#: ../../source/style_properties.rst:615
msgid "The window scaling factor is always above 1."
msgstr ""

#: ../../source/style_properties.rst:622
msgid "``\"linear\"``"
msgstr ""

#: ../../source/style_properties.rst:618
msgid "Best for text with thick outlines. The window scaling factor is applied to the outline size, and then rounded to an integer. This looks better for thick outlines, but small outlines can dissappear."
msgstr ""

#: ../../source/style_properties.rst:623
msgid "The resulting width is always at least 1 pixel."
msgstr ""

#: ../../source/style_properties.rst:627
msgid "Specifies the number of pixels the second and later lines in a paragraph are indented by."
msgstr ""

#: ../../source/style_properties.rst:632
msgid "If not None, this should be a style object. The style that's used for ruby text."
msgstr ""

