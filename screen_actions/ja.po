msgid "Preference(\"text speed\")"
msgstr "Preference(\"text speed\")"

#: ../../source/inc/preference_action:164
msgid "Preference(\"auto-forward time\")"
msgstr "Preference(\"auto-forward time\")"

#: ../../source/inc/preference_action:165
msgid "Preference(\"main volume\")"
