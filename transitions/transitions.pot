msgid "`control`"
msgstr ""

#: ../../source/inc/transition:11
msgid "The control transform."
msgstr ""

#: ../../source/inc/transition:14
#: ../../source/inc/transition:189
msgid "`delay`"
msgstr ""

#: ../../source/inc/transition:14
msgid "The time the transition takes, before ending."
msgstr ""

